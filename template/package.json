{
  "name": "rnbetterboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "patch": "patch-package",
    "iphone11": "react-native run-ios --simulator='iPhone 11'",
    "iphone8": "react-native run-ios --simulator='iPhone 8'",
    "iphone6": "react-native run-ios --simulator='iPhone 6'",
    "iphone5s": "react-native run-ios --simulator='iPhone 5s'",
    "postinstall": "patch-package && node ./scripts/post-install",
    "format": "prettier -write --ignore-unknown ./src/**/*.{js,jsx,ts,tsx,json,md}",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "clean:android": "cd ./android/ && ./gradlew clean && rm -fr ../android/detox/build/",
    "prepare": "husky install",
    "ios:beta": "cd ios && bundle exec fastlane ios distribute_to_testflight",
    "android:beta": "cd android && bundle exec fastlane android distribute_to_playstore",
    "test:ios-debug:reuse": "detox test -c ios.sim.debug --reuse",
    "test:ios-debug": "detox test -c ios.sim.debug",
    "test:ios-release": "detox test -c ios.sim.release",
    "test:android-debug": "detox test -c android.emu.debug",
    "test:android-release": "detox test -c android.emu.release",
    "e2e:ios-debug": "yarn build:os-debug && yarn test:ios-debug",
    "e2e:android-debug": "yarn build:android-debug && yarn test:android-debug",
    "e2e:android-release": "yarn build:android-release && yarn test:android-release",
    "build:ios-debug": "detox build -c ios.sim.debug",
    "build:ios-release": "detox build -c ios.sim.release",
    "build:android-debug": "detox build -c android.emu.debug",
    "build:android-release": "detox build -c android.emu.release",
    "lint:deadcode": "ts-prune"
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "^3.1.2",
    "@gorhom/bottom-sheet": "^4.3.2",
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/netinfo": "^9.0.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "@rematch/core": "^2.2.0",
    "@rematch/immer": "^2.1.3",
    "@rematch/loading": "^2.1.2",
    "@rematch/persist": "^2.1.2",
    "@rematch/select": "^3.1.2",
    "@rneui/base": "^4.0.0-rc.4",
    "@rneui/themed": "^4.0.0-rc.4",
    "@shopify/restyle": "^2.1.0",
    "formik": "^2.2.9",
    "i18next": "^21.8.9",
    "immer": "^9.0.15",
    "intl-pluralrules": "^1.3.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash.memoize": "^4.1.2",
    "react": "17.0.2",
    "react-i18next": "^11.17.2",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.6",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.2.2",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.1.0",
    "react-native-webview": "^11.22.2",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "use-immer": "^0.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.12",
    "@types/react-native": "^0.67.8",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^28.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^19.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-module-resolver": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "metro-react-native-babel-preset": "^0.71.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.0",
    "react-native-flipper": "^0.146.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "^18.1.0",
    "redux-flipper": "^2.0.1",
    "solidarity": "^3.0.4",
    "ts-jest": "^28.0.5",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
